<table>
   <tr>
     <th>Board Name</th>
     <th>Height</th>
     <th>Color</th>
   </tr>
   <% @surfboards.each do |surfboard| %>
   <tr>
     <td><%= surfboard.name %></td>
     <td><%= surfboard.full_height %></td>
     <td><%= surfboard.color %></td>
   </tr>
   <% end %>
 </table> 

 <%= link_to "Create New", "/surfboards/new", method: :get %>


